<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>StartPage.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>com.netcracker.students.BatyrkinAndrew.BatyrkinAndrew BatyrkinAndrewService</servlet-name>
        <servlet-class>com.netcracker.students.BatyrkinAndrew.server.BatyrkinAndrewServiceImpl</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>com.netcracker.students.BatyrkinAndrew.BatyrkinAndrew DeleteBookService</servlet-name>
        <servlet-class>com.netcracker.students.BatyrkinAndrew.server.DeleteBookServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>com.netcracker.students.BatyrkinAndrew.BatyrkinAndrew GetTableService</servlet-name>
        <servlet-class>com.netcracker.students.BatyrkinAndrew.server.GetTableServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>com.netcracker.students.BatyrkinAndrew.BatyrkinAndrew SortByTitleService</servlet-name>
        <servlet-class>com.netcracker.students.BatyrkinAndrew.server.SortByTitleServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>com.netcracker.students.BatyrkinAndrew.BatyrkinAndrew AddNewBookService</servlet-name>
        <servlet-class>com.netcracker.students.BatyrkinAndrew.server.AddNewBookServiceImpl</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>com.netcracker.students.BatyrkinAndrew.BatyrkinAndrew BatyrkinAndrewService</servlet-name>
        <url-pattern>/BatyrkinAndrew/BatyrkinAndrewService</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>com.netcracker.students.BatyrkinAndrew.BatyrkinAndrew DeleteBookService</servlet-name>
        <url-pattern>/BatyrkinAndrew/DeleteBookService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.netcracker.students.BatyrkinAndrew.BatyrkinAndrew GetTableService</servlet-name>
        <url-pattern>/BatyrkinAndrew/GetTableService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.netcracker.students.BatyrkinAndrew.BatyrkinAndrew SortByTitleService</servlet-name>
        <url-pattern>/BatyrkinAndrew/SortByTitleService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.netcracker.students.BatyrkinAndrew.BatyrkinAndrew AddNewBookService</servlet-name>
        <url-pattern>/BatyrkinAndrew/AddNewBookService</url-pattern>
    </servlet-mapping>


</web-app>